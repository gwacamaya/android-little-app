<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.helloapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
            android:usesCleartextTraffic="true"
            android:allowBackup="true"
            android:icon="@mipmap/errors"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/errors"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activities.FaireCourses"
                  android:label="Faire les courses"/>
        <activity android:name=".activities.AcheterProduit"
                  android:label="Acheter un produit"/>
    </application>

</manifest>